<%- include ('../partials/header') %>
    <%- include ('../partials/menu') %>

        <main>
            <div class="container">
                <%- include ('../partials/message') %>
                <table style="width:100%">
                    <tr>
                        <th>Username</th>
                        <th>Score</th>
                    </tr>
                    <% people.forEach(function(user) { %>
                        <tr>
                            <td><%= user.username %></td>
                            <td><%= user.highscore %></td>
                        </tr>
                    <% }); %>
                </table>
            </div>
        </main>

